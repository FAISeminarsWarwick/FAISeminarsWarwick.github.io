---
layout: default
---
<link rel="stylesheet" href="{{ '/assets/css/styles.css' | relative_url }}"> 

<div class="speaker-page">
  <div class="container">
    <h1>{{ page.title }}</h1>
    <h2>{{ page.position }}</h2>
    <img src="{{ page.image }}" alt="{{ page.title }}" class="img-fluid rounded-circle align-left">
    
    <div class="speaker-content">
      {{ content }}
    </div>
  </div>
</div>


